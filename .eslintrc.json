{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "strict": 0,
        "quotes": [
            2,
            "single"
        ],
        "no-underscore-dangle": 0,
        "no-unused-vars": 1,
        "no-unused-expressions": 0,
        "react/jsx-no-undef": 2,
        "new-cap": 0,
        "yoda": 0,
        "no-empty": 0,
        "no-extra-boolean-cast": 0,
        "consistent-return": 0,
        "react/jsx-uses-vars": 1
    }
}